{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.StaticAssets.StaticAssetsInvoker": "Error",
      "BobFx.Core.Services": "Debug"
    }
  },
  "AllowedHosts": "*",
  "LedConfiguration": {
    "LedCount": 30
  },
  "UdpConfiguration": {
    "TargetAddress": "255.255.255.255",
    "TargetPort": 21324,
    "UpdateIntervalMs": 10
  },
  "CountdownConfiguration": {
    "DurationSeconds": 600,
    "DeviationSeconds": 120
  }
}
