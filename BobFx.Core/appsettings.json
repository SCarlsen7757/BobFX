{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.StaticAssets.StaticAssetsInvoker": "Error",
      "BobFx.Core.Services": "Debug"
    }
  },
  "AllowedHosts": "*",
  "WLed": {
    "Led": {
      "LedCount": 30
    },
    "Udp": {
      "TargetAddress": "255.255.255.255",
      "TargetPort": 21324,
      "UpdateIntervalMs": 10
    },
    "Discovery": {
      "UpdateIntervalSeconds": 60
    }
  },
  "Countdown": {
    "PreDurationSeconds": 5,
    "DurationSeconds": 600,
    "DeviationSeconds": 120
  }
}
